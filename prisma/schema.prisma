generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model twitt {
  id         Int      @id @default(autoincrement())
  createdAt  DateTime @default(now()) @db.Timestamp(6)
  content    String   @db.VarChar(255)
  authorAddr String   @db.VarChar(64)
}
